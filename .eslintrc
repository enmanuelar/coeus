{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "jsx-a11y",
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-undef": "off",
    "comma-dangle": [1, {
      "objects": "always"
    }],
    "space-before-function-paren": [2, "always"],
    "class-methods-use-this": [0]
  }
}
